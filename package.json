{
  "name": "tia",
  "version": "0.27.2",
  "description": "Time is All (logs driven test engine with ExtJs support)",
  "bin": {
    "tia": "build/bin/tia.js"
  },
  "engines": {
    "node": ">=10.15.1"
  },
  "scripts": {
    "clean": "rm -rf build/* && rm -rf build-types/*",
    "test": "node build/bin/tia.js --run-self-tests",
    "postinstall": "node_modules/.bin/tsc || exit 0",
    "a": "npm run clean && npm run postinstall && npm run test",
    "b": "npm run postinstall"
  },
  "types": "./tia-types/index.d.ts",
  "typings": "./tia-types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Dzenly/tia.git"
  },
  "keywords": [
    "log",
    "comparison",
    "test",
    "testing",
    "framework",
    "system",
    "engine",
    "selenium",
    "webdriver",
    "extjs",
    "web",
    "gui",
    "unit",
    "regression"
  ],
  "_moduleAliases": {
    "@root": "."
  },
  "author": "Dzenly <alexchem@yandex.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dzenly/tia/issues"
  },
  "homepage": "https://github.com/Dzenly/tia#readme",
  "dependencies": {
    "camelcase-keys": "^5.2.0",
    "chalk": "^2.4.1",
    "chromedriver": "^76.0.0",
    "debug-assert": "^0.2.3",
    "diff": "^4.0.1",
    "lodash": "^4.17.14",
    "minimist": "^1.2.0",
    "nodemailer": "^5.1.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "semver": "^5.7.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.138",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.6.1",
    "@types/selenium-webdriver": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  }
}
